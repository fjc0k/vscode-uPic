{
  "compilerOptions": {
    "strict": true,
    "target": "es2019",
    "module": "CommonJS",
    "moduleResolution": "node",
    "declaration": false,
    "removeComments": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "preserveSymlinks": true,
    "newLine": "LF",
    "jsx": "react",
    "outDir": "./dist",
    "lib": ["es2019"]
  }
}
